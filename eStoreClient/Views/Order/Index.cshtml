@model IEnumerable<BusinessObject.Models.Order>

<h2>Order List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Member ID</th>
            <th>Order Date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.OrderId</td>
                <td>@item.MemberId</td>
                <td>@item.OrderDate</td>
                <td>
                    <a class="btn btn-info" asp-action="Details" asp-route-id="@item.OrderId">Order Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
